    36: def self.sessions payload, params
    37:   person = Gabb::Person.find(payload["id"])
    38:   limit = params["limit"] ? params["limit"] : 25
    39:   episode_hashes = person.sessions.distinct("episode_hash")
    40:   limit = (episode_hashes.count < limit) ? episode_hashes.count : limit
    41: 
    42:   sessions = []
    43: 
 => 44:   binding.pry
    45: 
    46:   for i in 0..limit
    47:     episode_hash = episode_hashes[i]
    48:     sessions << person.sessions.where(episode_hash: episode_hash).order_by(created_at: 'desc').first
    49:   end
    50: 
    51:   binding.pry
    52: 
    53:   sessions
    54: end

